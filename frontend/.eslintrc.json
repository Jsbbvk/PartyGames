{
  "env": {
    "browser": true,
    "es2021": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["react", "babel"],
  "rules": {
    "no-unused-vars": 0,
    "no-console": 0,
    "react/react-in-jsx-scope": 0,
    "react/jsx-props-no-spreading": 0,
    "react/prop-types": 0,
    "no-underscore-dangle": 0,
    "jsx-a11y/label-has-associated-control": [
      "error",
      {
        "required": {
          "some": ["nesting", "id"]
        }
      }
    ],
    "jsx-a11y/label-has-for": [
      "error",
      {
        "required": {
          "some": ["nesting", "id"]
        }
      }
    ],
    "jsx-a11y/click-events-have-key-events": 0,
    "jsx-a11y/no-noninteractive-element-interactions": 0,
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto",
        "tabWidth": 2
      }
    ]
  },
  "extends": ["airbnb", "plugin:prettier/recommended"]
}
